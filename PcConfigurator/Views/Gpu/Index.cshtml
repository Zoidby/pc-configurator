@model PcConfigurator.Models.GpuModels.GpuIndexModel

@{
    ViewBag.Title = "Cpus";
}
<br/>
@if (Model.GpuList.Count > 0)
{
    <table class="table table-striped">
        <thead>
        <tr>
            <th>Brand</th>
            <th>Model</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var gpu in Model.GpuList)
        {
            <tr>
                <td>@gpu.Brand</td>
                <td>@gpu.Name</td>
            </tr>
        }
        </tbody>
    </table>
}